            <a href="@Url.Action("Icons", "Plant")">
                <div class="index-table-image"><img src="@Url.Content("~/Images/Icons/PlantIcons.png")" alt="@Resource.PlantIcons" /></div>
                <div class="index-table-label">@Resource.PlantIcons</div>
            </a>
        </td>
        <td class="index-table-cell">
            <a href="@Url.Action("HomeIcons", "Plant")">
                <div class="index-table-image"><img src="@Url.Content("~/Images/Icons/HomeIcons.png")" alt="@Resource.HomeIcons" /></div>
                <div class="index-table-label">@Resource.HomeIcons</div>
            </a>
        </td>
    </tr>
</table>